            id="MoSaRTBackEnd_AnalysisRepository.presentation.MoSaRTBackEnd_AnalysisRepositoryEditorID"
            name="%_UI_MoSaRTBackEnd_AnalysisRepositoryEditor_label"
            icon="icons/full/obj16/MoSaRTBackEnd_AnalysisRepositoryModelFile.gif"
            extensions="mosartbackend_analysisrepository"
            class="MoSaRTBackEnd_AnalysisRepository.presentation.MoSaRTBackEnd_AnalysisRepositoryEditor"
            contributorClass="MoSaRTBackEnd_AnalysisRepository.presentation.MoSaRTBackEnd_AnalysisRepositoryActionBarContributor">
      </editor>
   </extension>

