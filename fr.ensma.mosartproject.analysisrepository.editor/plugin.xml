<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="MoSaRTBackEnd_AnalysisRepository.presentation.MoSaRTBackEnd_AnalysisRepositoryModelWizardID"
            name="%_UI_MoSaRTBackEnd_AnalysisRepositoryModelWizard_label"
            class="MoSaRTBackEnd_AnalysisRepository.presentation.MoSaRTBackEnd_AnalysisRepositoryModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MoSaRTBackEnd_AnalysisRepositoryModelFile.gif">
