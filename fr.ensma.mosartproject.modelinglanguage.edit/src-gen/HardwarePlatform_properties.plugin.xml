<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.ensma.fr/MoSaRT/HardwarePlatform"
            class="MoSaRT.HardwarePlatform.providers.HardwarePlatformEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpUtilityTypes.providers.RtpUtilityTypesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpComputationAndAnalysisFunctions.providers.RtpComputationAndAnalysisFunctionsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.FunctionalElements.providers.FunctionalElementsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.SoftwarePlatform.SoftwareOperators.providers.SoftwareOperatorsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.providers.MoSaRTPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpTypes.providers.RtpTypesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.HardwarePlatform.providers.HardwarePlatformPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.providers.RealTimePropertiesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.SoftwarePlatform.SoftwareBehavior.providers.SoftwareBehaviorPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpMeasurementUnits.providers.RtpMeasurementUnitsPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpProtocolsAndPolicies.providers.RtpProtocolsAndPoliciesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="MoSaRT.RealTimeProperties.RtpHelperTypes.providers.RtpHelperTypesPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="MoSaRT.HardwarePlatform.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="MoSaRT.HardwarePlatform.properties">
         <propertyTab
               id="Base"
               label="Base"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="MoSaRT.HardwarePlatform.properties">
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.SystemHardwareSidePropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.SystemHardwareSidePropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.SystemHardwareSide"
               tab="Base">
         </propertySection>
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.HpProcessingUnitPropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.HpProcessingUnitPropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.HpProcessingUnit"
               tab="Base">
         </propertySection>
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.HpCommunicationRouterPropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.HpCommunicationRouterPropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.HpCommunicationRouter"
               tab="Base">
         </propertySection>
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.HpNetworkPortPropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.HpNetworkPortPropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.HpNetworkPort"
               tab="Base">
         </propertySection>
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.HpCommunicationChannelPropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.HpCommunicationChannelPropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.HpCommunicationChannel"
               tab="Base">
         </propertySection>
         <propertySection
               class="MoSaRT.HardwarePlatform.parts.forms.HpProcessorInterconnectorPropertiesEditionPartForm"
               filter="MoSaRT.HardwarePlatform.providers.HpProcessorInterconnectorPropertiesEditionProvider$EditionFilter"
               id="MoSaRT.HardwarePlatform.section.HpProcessorInterconnector"
               tab="Base">
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
